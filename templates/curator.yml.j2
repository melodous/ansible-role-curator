---
client:
  hosts:
    - {{ curator_es_node }}
  port: 9200
  url_prefix:
  use_ssl: False
  certificate:
  client_cert:
  client_key:
  aws_key:
  aws_secret_key:
  aws_region:
  ssl_no_validate: False
  http_auth:
  timeout: 30
  master_only: False

logging:
  loglevel: {{ curator_loglevel }}
  logfile: {{ curator_logfile }}
  logformat: {{ curator_logformat }}
  blacklist: ['elasticsearch', 'urllib3']

